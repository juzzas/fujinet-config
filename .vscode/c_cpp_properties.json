{
    "configurations": [
        {
            "name": "cc65 Apple II",
            "includePath": [
                "/usr/share/cc65/include/**",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "BUILD_APPLE2"
            ],
            "compilerPath": "/usr/bin/cl65",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "${default}"
        },
        {
            "name": "cc65 Atari",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/share/cc65/include/**"
            ],
            "defines": [
                "BUILD_ATARI"
            ],
            "compilerPath": "/usr/bin/cc65",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "${default}"
        },
        {
            "name": "z88dk ADAM",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/local/share/z88dk/include/**",
                "../eoslib/**",
                "../smartkeyslib/**"
            ],
            "defines": [
                "BUILD_ADAM"
            ],
            "compilerPath": "/usr/local/bin/zcc",
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "${default}"
        }
    ],
    "version": 4
}